{
  "name": "gearfall",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "dev:web": "pnpm --filter @gearfall/web dev",
    "dev:api": "pnpm --filter @gearfall/api dev",
    "db:migrate": "pnpm --filter @gearfall/api db:migrate",
    "db:seed": "pnpm --filter @gearfall/api db:seed"
  }
}
